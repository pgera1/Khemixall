{
  "version": 2,
  "builds": [{ "src": "frontend/package.json", "use": "@vercel/static-build" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://kemixall-backend.onrender.com/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "REACT_APP_API_URL": "https://kemixall-backend.onrender.com"
  }
}
